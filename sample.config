listen_port=55555
max_messages=10485760
high_watermark=90
low_watermark=50
# `reject` or `drop`
overflow_policy=reject
message_ttl_sec=3600
publish_maxiters=1024
subscribe_maxiters=0
router_threads=2
reactor_threads=2
worker_threads=4

# 4:info, 5:debug
log_level=4
log_procname=broker
log_filename=log

debug_seqid=false
